<web-app xmlns="http://xmlns.jcp.org/xml/ns/javaee"
  xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
  xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee
                      http://xmlns.jcp.org/xml/ns/javaee/web-app_3_1.xsd"
  version="3.1"
  metadata-complete="true">

    <description>
      Sistema de Votação
    </description>
    <display-name>Sistema de Votação</display-name>
    
    <servlet>
        <servlet-name>Login</servlet-name>
        <servlet-class>controller.Login</servlet-class>
    </servlet>
    
    <servlet-mapping>
        <servlet-name>Login</servlet-name>
        <url-pattern></url-pattern>
    </servlet-mapping>
    
    <servlet>
        <servlet-name>Autenticador</servlet-name>
        <servlet-class>controller.Autenticador</servlet-class>
    </servlet>
    
    <servlet-mapping>
        <servlet-name>Autenticador</servlet-name>
        <url-pattern>/Home</url-pattern>
    </servlet-mapping>

    <servlet>
        <servlet-name>HabilitarVoto</servlet-name>
        <servlet-class>controller.HabilitarVoto</servlet-class>
    </servlet>

    <servlet-mapping>
        <servlet-name>HabilitarVoto</servlet-name>
        <url-pattern>/HabilitarVoto</url-pattern>
    </servlet-mapping>

    <servlet>
        <servlet-name>Votacao</servlet-name>
        <servlet-class>controller.Votacao</servlet-class>
    </servlet>

    <servlet-mapping>
        <servlet-name>Votacao</servlet-name>
        <url-pattern>/Votar</url-pattern>
    </servlet-mapping>

    <servlet>
        <servlet-name>Relatorio</servlet-name>
        <servlet-class>controller.Relatorio</servlet-class>
    </servlet>

    <servlet-mapping>
        <servlet-name>Relatorio</servlet-name>
        <url-pattern>/Relatorio</url-pattern>
    </servlet-mapping>
    
</web-app>

